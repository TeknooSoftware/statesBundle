{
    "name": "unialteri/states-bundle",
    "type": "symfony-bundle",
    "description": "Bundle of UniAlteri States to use it easily with Symfony 2 + Doctrine",
    "keywords": ["states","class","symfony","bundle"],
    "homepage": "http://teknoo.it/states",
    "license": ["MIT","GPL-3.0+"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "r.deloge@uni-alteri.com",
            "role":"Software architect, Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": ">=2.3,<3.0",
        "symfony/console": ">=2.3,<3.0",
        "symfony/validator": ">=2.3,<3.0",
        "doctrine/orm": ">=2.2,<3.0",
        "unialteri/states": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0,<5.0",
        "phpunit/phpunit-mock-objects": "*",
        "phpunit/phpunit-selenium": "*",
        "phpunit/phpunit-story": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "UniAlteri\\Bundle\\StatesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniAlteri\\Tests\\Bundle\\StatesBundle\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    }
}
